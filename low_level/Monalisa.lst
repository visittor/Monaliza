....................  
.................... void PIDPositionalForm(float error, PIDParameter *MOTOR) 
.................... { 
.................... 	MOTOR->errorSummation += error; 
.................... 	MOTOR->previousError = error; 
....................  
.................... 	MOTOR->u = (MOTOR->Kp * error) + (MOTOR->Ki * MOTOR->errorSummation) + (MOTOR->Kd*(error - MOTOR->previousError)); 
.................... } 
